{
  "version": 2,
  "buildCommand": "bun run build",
  "devCommand": "bun run dev",
  "installCommand": "bun install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["all"],
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920, 2048, 3840],
    "domains": [
      "source.unsplash.com",
      "images.unsplash.com",
      "ext.same-assets.com",
      "ugc.same-assets.com"
    ],
    "formats": ["image/avif", "image/webp"],
    "minimumCacheTTL": 60
  },
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://swiftburst-food.vercel.app",
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  }
}
